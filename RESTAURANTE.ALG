algoritmo "restaurante"
  var prato, pesoTotal, peso : inteiro
var vlrKilo, vlrTotal : real
 inicio
  vlrKilo <- 20
 prato <- -230
   escreval ("digite o peso total do prato")
leia pesoTotal
peso <- ( pesoTotal+prato)
 vlrTotal <- ( peso*vlrKilo)
  escreval("O valor total a ser pago é: $", vlrTotal)
  fimalgoritmo